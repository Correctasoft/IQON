  <script>
    //search in tables
    function searchInTable(ele, tableid) {
      var value = $(ele).val().toLowerCase();
      $("#" + tableid + " tr").filter(function () {
        $(this).toggle($(this).text().toLowerCase().indexOf(value) > -1)
      });
    }
