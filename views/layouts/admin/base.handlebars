  <script>
    //date print
    function getTodayDate(target, isHTMLelement) {
      var today = new Date();
      if (isHTMLelement) {
        document.getElementById(target).innerHTML = today.getFullYear() + '-' + ('0' + (today.getMonth() + 1)).slice(-2) + '-' + ('0' + today.getDate()).slice(-2);
      }
      else {
        document.getElementById(target).value = today.getFullYear() + '-' + ('0' + (today.getMonth() + 1)).slice(-2) + '-' + ('0' + today.getDate()).slice(-2);
      }
    }

